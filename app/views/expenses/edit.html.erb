<h1>Editing expense</h1>

<% form_for(@expense) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :cluster_name %><br />
    <%= f.text_field :cluster_name %>
  </p>
  <p>
    <%= f.label :shop_name %><br />
    <%= f.text_field :shop_name %>
  </p>
  <p>
    <%= f.label :date %><br />
    <%= f.date_select :date %>
  </p>
  <p>
    <%= f.label :total %><br />
    <%= f.text_field :total %>
  </p>
  <p>
    <%= f.label :adjustable_total %><br />
    <%= f.text_field :adjustable_total %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @expense %> |
<%= link_to 'Back', expenses_path %>