<h1>Listing expense_perticulers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Expense type</th>
  </tr>

<% @expense_perticulers.each do |expense_perticuler| %>
  <tr>
    <td><%=h expense_perticuler.name %></td>
    <td><%=h expense_perticuler.expense_type %></td>
    <td><%= link_to 'Show', expense_perticuler %></td>
    <td><%= link_to 'Edit', edit_expense_perticuler_path(expense_perticuler) %></td>
    <td><%= link_to 'Destroy', expense_perticuler, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New expense_perticuler', new_expense_perticuler_path %>